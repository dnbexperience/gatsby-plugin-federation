  "author": "Tobias HÃ¸egh <tobias.hoegh@dnb.no>",
  "scripts": {
    "start": "cypress open",
    "test:ci": "yarn workspace gatsby-plugin-federation build && yarn test:develop && yarn test:production",
    "host:develop": "yarn workspace host start",
    "host:production": "yarn workspace host build && yarn workspace host serve",
    "remote:develop": "yarn workspace remote start",
    "remote:production": "yarn workspace remote build && yarn workspace remote serve",
    "test:develop": "start-server-and-test host:develop 8001 remote:develop 8002 test:develop:run",
    "test:production": "start-server-and-test host:production 8001 remote:production 8002 test:production:run",
    "test:develop:run": "cypress run --record false --spec '**/*-develop.cy.ts'",
    "test:production:run": "cypress run --record false --spec '**/*-production.cy.ts'"
  },
  "devDependencies": {
    "cypress": "12.5.1",
    "start-server-and-test": "1.15.4"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
